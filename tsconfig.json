{
	"compilerOptions": {
		"noImplicitAny": false,
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "Bundler",
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"jsx": "react-jsx",
		"jsxImportSource": "lestin",
		"paths": {
			"lestin": ["./node_modules/lestin/jsx-runtime"],
		}
	}
}
